[Tests]
 #Note that max time is set high because qsub already has a timelimit.
 [./test_mpi]
   executable = 'qsub'
   parameters = '-P moose -l select=6:ncpus=4:mpiprocs=1 -l walltime=10:00:00 -l place=free -W block=true ./run_mpi_test.sh'
   #input=test_mpi.xml
   run_types = 'qsub'
   type = GenericExecutable
   output_wait_time = 60
   max_time = 3600
