<?xml version="1.0" encoding="UTF-8"?>
<Simulation verbosity='debug'>
<RunInfo>
    <WorkingDir>CreateInternalObjFromCSVs</WorkingDir>
    <Files>.</Files>
    <Sequence>LoadCsvIntoInternalObjectTestData,LoadCsvIntoInternalObjectTestHDF5</Sequence>
    <batchSize>1</batchSize>
</RunInfo>

<Models>
    <PostProcessor name="LoadCsvIntoInternalObjectTest" subType='LoadCsvIntoInternalObject'>
        <directory>../GridTestRaven/FirstMRun</directory>
    </PostProcessor>
</Models>

<Steps>
    <PostProcess name='LoadCsvIntoInternalObjectTestData'>
        <Model    class='Models'           type='PostProcessor'   >LoadCsvIntoInternalObjectTest</Model>
        <Output   class='DataObjects'            type='PointSet'    >PointSetPostProcTest</Output>
        <Output   class='OutStreamManager' type='Print'           >PointSetPostProcTest_DUMP</Output>
    </PostProcess>
    <PostProcess name='LoadCsvIntoInternalObjectTestHDF5'>
        <Model    class='Models'           type='PostProcessor'   >LoadCsvIntoInternalObjectTest</Model>
        <Output   class='Databases'        type='HDF5'            >HDF5PostProcTest</Output>
    </PostProcess>
</Steps>
<OutStreamManager>
    <Print name='PointSetPostProcTest_DUMP'>
        <type>csv</type>
        <source>PointSetPostProcTest</source>
    </Print>
</OutStreamManager>
<DataObjects>
    <PointSet name='PointSetPostProcTest'>
        <Input>Dummy1</Input>
        <Output>time,pipe_Dh,pipe_Tw</Output>
    </PointSet>
</DataObjects>
<Databases>
    <HDF5 name="HDF5PostProcTest" />
</Databases>

</Simulation>
