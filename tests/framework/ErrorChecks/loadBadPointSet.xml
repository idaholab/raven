<?xml version="1.0" ?>
<Simulation verbosity="debug">
  
  <RunInfo>
    <WorkingDir>badData</WorkingDir>
    <Sequence>readData</Sequence> 
  </RunInfo>

  <Files>
    <Input name="loadRawDataFile">badHistory.csv</Input>
  </Files>

  <DataObjects>
    <PointSet name="loadData">
      <Input>year,scaling_load</Input>
      <Output>seconds,load</Output>
    </PointSet>
  </DataObjects>

  <Steps>
    <IOStep name='readData' pauseAtEnd = 'True'>
      <Input  class='Files'       type=''>loadRawDataFile</Input>
      <Output class='DataObjects' type='PointSet'>loadData</Output>          
    </IOStep>
  </Steps>

</Simulation>
