<?xml version="1.0" ?>
<Simulation verbosity="debug">

  <RunInfo>
    <WorkingDir>badData</WorkingDir>
    <Sequence>readData</Sequence> 
  </RunInfo>

  <Files>
    <Input name="loadRawDataFile">badHistorySet.csv</Input>
  </Files>

  <DataObjects>
    <HistorySet name="loadData">
      <Input>year,scaling_load</Input>
      <Output>seconds,load</Output>
    </HistorySet>
  </DataObjects>

  <Steps>
    <IOStep name='readData' pauseAtEnd = 'True'>
      <Input  class='Files'       type=''>loadRawDataFile</Input>
      <Output class='DataObjects' type='HistorySet'>loadData</Output>          
    </IOStep>
  </Steps>
  
</Simulation>
