  <DataObjects>
    <PointSet name='LOCA_PS'>
        <Input>ACC1,ACC2,ACC3,
               HPI_A_time_act,HPI_B_time_act,HPI_C_time_act,
               HPR_A_time_act,HPR_B_time_act,HPR_C_time_act,
